{
  "name": "nodexer",
  "version": "1.1.6",
  "description": "A file directory indexer / lister script written in Node.",
  "main": "./index.js",
  "scripts": {
    "dev": "nodemon --config ./.nodemon.json ./run.js",
    "lint": "eslint ./src ./public/js",
    "compile": "npm-run-all transpile build",
    "build": "node ./build.js",
    "transpile": "babel -d ./lib/ ./src/"
  },
  "keywords": [
    "indexer",
    "directory",
    "lister",
    "gallery",
    "browser",
    "files",
    "listing"
  ],
  "author": "ему (sixem)",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.10.5",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-basic-auth": "^1.2.0",
    "lodash": "^4.17.19",
    "pug": "^3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/node": "^7.10.5",
    "@babel/plugin-transform-runtime": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "filehound": "^1.17.4",
    "chalk": "^4.1.0",
    "eslint": "^7.5.0",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "uglify-js": "^3.10.0",
    "uglifycss": "0.0.29"
  },
  "browserslist": "> 0.25%, not dead",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sixem/nodexer.git"
  },
  "bugs": {
